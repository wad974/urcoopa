{
  "manifest_version": 3,
  "name": "ODOO PAGE FACTURES URCOOPA",
  "version": "1.0",
  "description": "Ajoute un bouton pour afficher un lien facture adh√©rent urcoopa",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/html/facture-ocr.html",
        "assets/html/apprentissage-ocr.html",
        "assets/html/loading-page.html"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "assets/js/bouton.js",
        "assets/js/index.js",
        "assets/js/loadingPage.js"
      ]
    }
  ]
}
